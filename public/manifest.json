{
	"manifest_version": 3,
	"name": "Signup Subaddressing",
	"version": "1.0.0",
	"version_name": "v1.0.0 (2022-01-01)",
	"description": "Adds a hash to your email when signing up on websites",
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"permissions": ["storage", "activeTab"],
	"action": {
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "static/js/worker.js"
	}
}
